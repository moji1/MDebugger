            <transition xmi:type="uml:Transition" xmi:id="_qrr8oEj0EeaDOaB8rZNNSg" name="detecting" kind="internal" source="_U40GMEj0EeaDOaB8rZNNSg" target="_U40GMEj0EeaDOaB8rZNNSg">
              <ownedComment xmi:type="uml:Comment" xmi:id="_97WPcEj0EeaDOaB8rZNNSg">
